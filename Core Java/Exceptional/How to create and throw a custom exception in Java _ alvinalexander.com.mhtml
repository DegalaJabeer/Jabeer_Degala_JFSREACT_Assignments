.content-inner-right .front-page .views-field-title a:link, .content-inner-right .front-page .views-field-title a:visited, .content-inner-right .views-row article.node--view-mode-teaser h2 a:link, .content-inner-right .views-row article.node--view-mode-teaser h2 a:visited { font-family: "Lucida Grande", Arial, Verdana, sans-serif; font-size: 21px; letter-spacing: -0.03em; line-height: 1.3em; font-weight: bold; color: rgb(102, 102, 102); margin: 0.5em 0px -0.4em; padding-top: 0px; text-decoration: none; }

.path-frontpage .views-field-title { margin: 4em 8px 0.5em 0px; border-bottom: 2px dashed rgb(58, 120, 188); padding-bottom: 8px; line-height: 1.2em; }

.path-frontpage .views-field-title .field-content a, .path-frontpage .views-field-title .field-content a:link, .path-frontpage .views-field-title .field-content a:visited, .path-frontpage .views-field-title .field-content a:hover, .path-frontpage .views-field-title .field-content a:active { color: rgb(58, 120, 188); text-decoration: none; }

.content-inner-right h1.page-title { font-size: 26px; margin: 16px 0px 6px; color: rgb(99, 99, 99); }

.view-header .vocabulary-category .content .field--name-description p { font-size: 22px; font-style: italic; margin: 0px 0px 16px; padding: 0px; }

.content-inner-right .views-element-container .view-taxonomy-term .views-row { margin: 0px 0px 3em; padding: 0px; }

pre { width: 100%; }

pre.resizable { overflow: auto; resize: none; }

.similar, .recent-content { display: block; border-radius: 6px; background-color: rgb(254, 254, 254); border: 2px solid rgb(239, 239, 239); margin-top: 54px; margin-bottom: 2.5em; }

.recent-content { margin-top: 26px; }

.similar::before, .recent-content::before { display: block; padding: 0.8em 0px; border-radius: 4px 4px 0px 0px; text-align: center; margin: 0px 0px 1em; width: 100%; background: rgb(239, 239, 223); color: rgb(143, 140, 127); letter-spacing: 0.02em; font-weight: bold; font-size: 18px; }

.below_content .my_books { width: 100%; }

.below_content .my_books h2.title { display: block; width: 100%; }

.below_content .my_books table { width: 100%; }

.below_content .my_books a img { width: 110px; }

.pager { margin: 0px; padding: 4em 0px 4.5em; width: 100%; text-align: left; }

.pager li a:link { font-size: 1.3em; border-radius: 6px; padding: 0.6em 1em; margin-right: 0.4em; color: rgb(56, 99, 172); text-decoration: none; background: rgb(238, 238, 238); line-height: 3em; }

.page-node-type-source-code .content-inner-right h1.page-title, .content-inner-right .node--type-source-code h2, .content-inner-right .node--type-source-code h3, .page-node-type-blog .field--name-body h1, .page-node-type-blog .field--name-body h2, .page-node-type-blog .field--name-body h3 { color: rgb(99, 99, 99); }

.page-node-type-source-code .content-inner-right h1.page-title, .page-node-type-blog .field--name-body h1 { font-size: 24px; line-height: 1.4em; margin-bottom: 0.2em; margin-top: 15px; }

.page-node-type-source-code .content-inner-right h2, .page-node-type-blog .field--name-body h2 { font-size: 20px; padding: 1.4em 0px 0px; margin: 0px; line-height: 1.2em; }

.page-node-type-source-code .content-inner-right h3, .page-node-type-blog .field--name-body h3 { font-size: 16px; font-style: italic; padding: 1.5em 0px 0px; margin: 0px; line-height: 1.2em; }

.page-node-type-source-code .content-inner-right .field--name-body blockquote p, .page-node-type-blog .field--name-body blockquote p { margin-left: 2.5em; margin-right: 2.5em; text-align: justify; color: rgb(128, 128, 128); margin-bottom: 1.3em; line-height: 1.6em; }

.page-node-type-blog .field--name-category, .page-node-type-source-code .field--name-category { display: block; margin: 48px 0px 0.7em; }

