.page-node-type-text .content-inner-right .field--name-body, .page-node-type-misc .content-inner-right .field--name-body { border-radius: 12px; border: 2px dotted rgb(234, 237, 230); line-height: 1.6em; margin: 36px 4px 2em 0px; padding: 20px 24px 48px; background: rgb(255, 255, 255); }

.page-node-type-text .content-inner-right .field--name-body, .page-node-type-misc .content-inner-right .field--name-body { background-color: rgb(249, 250, 246); opacity: 0.95; }

